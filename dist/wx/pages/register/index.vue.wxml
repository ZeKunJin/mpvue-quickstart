<template name="2133f554">
  <view class="_div data-v-11fdf726 register">
    <image class="_img data-v-11fdf726 register-bg" src="/static/images/title-background.png"></image>
    <view class="_div data-v-11fdf726 block">
      <view class="_div data-v-11fdf726 title">
        <view class="_div data-v-11fdf726 desc"> 注册 </view>
      </view>
      <view style="margin-top: 30px" class="_div data-v-11fdf726">
        <view class="_div data-v-11fdf726 input" style="margin-top: 10px; position: relative"><input value="{{name}}" placeholder="请输入用户名" placeholder-class="placeholder" bindinput="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}" class="_input data-v-11fdf726" /></view>
        <view class="_div data-v-11fdf726 input" style="margin-top: 20px"><input value="{{phone}}" placeholder="请输入手机号" placeholder-class="placeholder" bindinput="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}" class="_input data-v-11fdf726" /></view>
        <view class="_div data-v-11fdf726 input" style="margin-top: 20px; position: relative"><input value="{{code}}" placeholder="请输入验证码" placeholder-class="placeholder" bindinput="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}" class="_input data-v-11fdf726" />
          <view wx:if="{{!disabled}}" class="_div data-v-11fdf726 code-text" bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}"> 发送验证码 </view>
          <view wx:else class="_div data-v-11fdf726 code-text" style="color: #999"> ({{ second }}s) </view>
        </view>
        <view style="margin-top: 20px; font-size: 12px" class="_div data-v-11fdf726">
          <van-checkbox value="{{checked}}" bindchange="handleProxy" checked-color="#E61F1C" icon-size="18" data-eventid="{{'5'}}" data-comkey="{{$k}}" class="_van-checkbox data-v-11fdf726"><label style="color: #666" class="_span data-v-11fdf726">我已阅读并同意</label> <label style="color: #E61F1C" bindtap="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}" class="_span data-v-11fdf726">《用户使用协议》</label></van-checkbox>
        </view>
      </view>
      <view class="_div data-v-11fdf726 button">
        <van-button open-type="getUserInfo" bindgetuserinfo="handleProxy" round color="linear-gradient(to right, #F15350, #F86E6E)" size="large" data-eventid="{{'6'}}" data-comkey="{{$k}}" class="_van-button data-v-11fdf726"> 注册 </van-button>
      </view>
      <view class="_div data-v-11fdf726 login-text" bindtap="handleProxy" data-eventid="{{'7'}}" data-comkey="{{$k}}"> 已有账号? <label style="color: #E61F1C" class="_span data-v-11fdf726">立即登录</label></view>
    </view>
  </view>
</template>