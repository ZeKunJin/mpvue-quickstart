<template name="1802efb2">
  <view class="_div data-v-7aa53857 require">
    <van-tabs active="{{active}}" border="{{false}}" color="#E61F1C" bindchange="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}" class="_van-tabs data-v-7aa53857">
      <van-tab wx:key="index" key="{{index}}" name="{{stair.id}}" title="{{stair.remark}}" wx:for="{{stairs}}" wx:for-index="index" wx:for-item="stair" class="_van-tab data-v-7aa53857">
        <view class="_div data-v-7aa53857 sidebar">
          <van-sidebar active-key="{{activityKey}}" bindchange="handleProxy" data-eventid="{{'0_'+index}}" data-comkey="{{$k}}" class="_van-sidebar data-v-7aa53857">
            <van-sidebar-item wx:key="_index" key="{{_index}}" title="{{item.label}}" wx:for="{{detailList}}" wx:for-index="_index" wx:for-item="item" class="_van-sidebar-item data-v-7aa53857"></van-sidebar-item>
          </van-sidebar>
        </view>
        <view class="_div data-v-7aa53857 container">
          <view wx:if="{{requirementList.length!==0}}" class="_ul data-v-7aa53857">
            <view wx:key="____index" key="{{____index}}" class="_li data-v-7aa53857 list_box" catchtap="handleProxy" data-eventid="{{'3_'+index+'-'+____index}}" data-comkey="{{$k}}" wx:for="{{requirementList}}" wx:for-index="____index" wx:for-item="itemss">
              <view class="_div data-v-7aa53857 list_image_box">
                <image src="{{itemss.cover}}" class="_img data-v-7aa53857"></image>
                <view class="_div data-v-7aa53857 urgent_box" wx:if="{{itemss.level === 2}}"> 急缺 </view>
              </view>
              <view class="_div data-v-7aa53857 list_right_box">
                <view class="_div data-v-7aa53857 first_box">
                  <view style="word-wrap:break-word;" class="_p data-v-7aa53857">{{itemss.title}}</view>
                  <van-button icon="weapp-nav" round size="mini" color="#F7F8FA" custom-style="color:#999999" catchtap="handleProxy" open-type="share" data-eventid="{{'1_'+index+'-'+____index}}" data-comkey="{{$k}}" class="_van-button data-v-7aa53857">分享 </van-button>
                </view>
                <view class="_div data-v-7aa53857 last_box"><label class="_span data-v-7aa53857">{{itemss.createTime}}发布</label>
                  <van-button round size="small" color="#F68725" custom-style="font-size:12px;height:44rpx" catchtap="handleProxy" data-eventid="{{'2_'+index+'-'+____index}}" data-comkey="{{$k}}" class="_van-button data-v-7aa53857">我要对接 </van-button>
                </view>
              </view>
            </view>
            <view style="text-align: center" hidden="{{!(!more)}}" class="_p data-v-7aa53857">没有更多数据</view>
          </view>
          <view class="_div data-v-7aa53857 empty_box" wx:else>
            <image src="/static/images/empty.png" class="_img data-v-7aa53857"></image> 暂无数据
          </view>
        </view>
      </van-tab>
    </van-tabs>
  </view>
</template>